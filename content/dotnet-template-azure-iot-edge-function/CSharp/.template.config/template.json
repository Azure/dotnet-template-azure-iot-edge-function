{
    "$schema": "http://json.schemastore.org/template",
    "author": "Microsoft",
    "classifications": [
        "Console"
    ],
    "name": "Azure IoT Edge Function",
    "identity": "Azure.IoT.Edge.Function.CSharp",
    "groupIdentity": "Azure.IoT.Edge.Function",
    "shortName": "aziotedgefunction",
    "tags": {
        "language": "C#",
        "type": "project"
    },
    "preferNameDirectory": true,
    "sourceName": "SampleFunction",
    "primaryOutputs": [
        {
            "path": "."
        }
    ],
    "symbols": {
        "repository": {
            "type": "parameter",
            "defaultValue": "<registry>/<repo-name>",
            "replaces": "<repository>"
        }
    },
    "sources": [
        {
            "exclude": [
                ".template.config/*"
            ]
        }
    ]
}
